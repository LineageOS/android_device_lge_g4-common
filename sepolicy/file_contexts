# SSD partitions
/dev/block/mmcblk0p32                       u:object_r:encrypt_block_device:s0
/dev/block/mmcblk0p35                       u:object_r:frp_block_device:s0
/dev/block/mmcblk0p40                       u:object_r:system_block_device:s0
/dev/block/mmcblk0p48                       u:object_r:cust_block_device:s0

# Device types
/dev/pn547                                  u:object_r:nfc_device:s0
/dev/rfkill                                 u:object_r:rfkill_device:s0
/dev/diag_lge                               u:object_r:lge_diag_device:s0
/dev/ttyHSL1                                u:object_r:cir_device:s0

# Sensors
/sns                                        u:object_r:sensors_data_file:s0
/sns/cal(/.*)?                              u:object_r:sensors_data_file:s0

# Socket types

